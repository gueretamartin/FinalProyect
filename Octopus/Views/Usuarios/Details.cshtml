@using Octopus.Models
@model Octopus.Models.VEHICULOS
@using System.Web.UI.WebControls

@{
    ViewBag.Title = "Vehículos - Detalles";
    Layout = "~/Views/Shared/_HomePanel.cshtml";
}


<!-- pageContent -->
<section class="full-width pageContent">

    <div>
        <div class="mdl-tabs__tab-bar">
            <a href="@Url.Action("Create","Vehiculos")" class="mdl-tabs__tab">NUEVO</a>
            <a href="@Url.Action("List", "Vehiculos")" class="mdl-tabs__tab">LISTA</a>
            <a href="@Url.Action("List", "Vehiculos")" class="mdl-tabs__tab" style="color: #2e6da4">DETALLES</a>
        </div>
        <div class="mdl-tabs__panel is-active" id="tabNewAdmin">
            <div class="mdl-grid">
                <div class="mdl-cell mdl-cell--4-col-phone mdl-cell--8-col-tablet mdl-cell--12-col-desktop">
                    <div class="full-width panel mdl-shadow--2dp">
                        <div class="full-width panel-tittle bg-primary text-center tittles">
                            @Model.VEH_PAT_MARCA_MODELO
                        </div>
                        <div class="full-width panel-content">

                            @using (Html.BeginForm("Acciones", "Vehiculos", new { veh_id = @Model.VEH_ID }, FormMethod.Post, new { enctype = "multipart/form-data" }))
                            {
                                @Html.AntiForgeryToken()
                                @Html.ValidationSummary(true)
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_PATENTE)</div>
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_MARCA)</div>
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_MODELO)</div>
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_CILINDRADAS)</div>
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_COLOR)</div>
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_KILOMETROS)</div>
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_AÑO)</div>
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_TIPOVEHICULO)</div>
                                <div class="text-condensedLight" style="text-align: center; color: black; background-color: #ff4d4d; width:40%; margin-left:30%; margin-right:30%;">@Html.ValidationMessageFor(a => a.VEH_PRECIO_INGRESO)</div>
                                <div class="mdl-grid">
                                    <div class="mdl-cell mdl-cell--4-col-phone mdl-cell--8-col-tablet mdl-cell--6-col-desktop">
                                        <h5 class="text-condensedLight" style="color: #2e6da4">SUCURSAL Y CLIENTE</h5>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.SUCURSALES.SUC_DESCRIP, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "1", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Sucursal</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.CLIENTES.CLI_LOCALIDAD, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "1", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Localidad</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.EMPLEADOS.EMP_APELLIDO_NOMBRE, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "1", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Empleado</label>
                                        </div>
                                        <h5 class="text-condensedLight mdl-textfield--floating-label" style="color: #2e6da4">VEHICULO</h5>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_PATENTE, new { @class = "mdl-textfield__input", @typeof = "number", tabindex = "3", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Patente</label>

                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_MODELO, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "5", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Modelo</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_CILINDRADAS, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "14", Value = String.Format("{0:n0}", Model.VEH_CILINDRADAS), disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Cilindradas</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_KILOMETROS, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "14", Value = String.Format("{0:n0}", Model.VEH_KILOMETROS), disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Kilometros</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_COLOR, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "11", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Color</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_PUERTAS, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "13", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Puertas</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_DETALLES, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "15", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Detalles</label>
                                        </div>
                                        <h5 class="text-condensedLight" style="color: #2e6da4">STEREO</h5>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_STEREO_MODELO, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "17", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">MODELO</label>
                                        </div>
                                        <h5 class="text-condensedLight" style="color: #2e6da4">VARIOS</h5>
                                        <br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_CIERRE, new { @disabled = "disabled", @Style = "color:dimgrey", @class = "zmdi zmdi-check-circl" }) Cierre &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_LLANTAS, new { tabindex = "18", disabled = "disabled", @Style = "color:dimgrey" }) LLantas &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_AIRBAG, new { tabindex = "19", disabled = "disabled", @Style = "color:dimgrey" }) Airabg &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_AIRE, new { tabindex = "20", disabled = "disabled", @Style = "color:dimgrey" }) Aire &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_LEVANTA_VIDRIOS, new { tabindex = "21", disabled = "disabled", @Style = "color:dimgrey" }) Levanta Vidrios &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_ESPEJOS_AUTOM, new { tabindex = "22", disabled = "disabled", @Style = "color:dimgrey" }) Espejos Automáticos &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_ASIENTOS_CUERO, new { tabindex = "23", disabled = "disabled", @Style = "color:dimgrey" }) Asientos de Cuero &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_FRENOS_ABS, new { tabindex = "24", disabled = "disabled", @Style = "color:dimgrey" }) Frenos ABS &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_NEBLINEROS, new { tabindex = "25", disabled = "disabled", @Style = "color:dimgrey" }) Neblineros &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                        <label class="mdl-radio" for="option-1" style="color: dimgrey">
                                            @Html.CheckBoxFor(model => model.VEH_RUEDA_AUXILIAR, new { tabindex = "26", disabled = "disabled", @Style = "color:dimgrey" }) Rueda Auxiliar &nbsp;&nbsp;&nbsp;
                                        </label><br />
                                    </div>
                                    <div class="mdl-cell mdl-cell--4-col-phone mdl-cell--8-col-tablet mdl-cell--6-col-desktop">
                                        <h5 class="text-condensedLight">  &nbsp;</h5>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.CLIENTES.CLI_CF_DNI_APELLIDO_NOMBRE, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "14", Value = String.Format("{0:n0}", Model.CLIENTES.CLI_CF_DNI_APELLIDO_NOMBRE), disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Cliente</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.CLIENTES.CLI_TELEFONO, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "2", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Teléfono Cliente</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.FECHAS.FEC_FECHA, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "14", Value = String.Format("{0:dd/MM/yyyy}", Model.FECHAS.FEC_FECHA), disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Fecha de Ingreso</label>
                                        </div>
                                        <h5 class="text-condensedLight">  &nbsp;</h5>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_MARCA, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "4", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Marca</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_VERSION, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "6", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Versión</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_TIPOVEHICULO, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "8", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Tipo de Vehículo</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_AÑO, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "10", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Año</label>
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_TIPOCOMBUSTIBLE, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "12", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Combustible</label>                                      
                                        </div>
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_PRECIO_INGRESO, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "14", Value = String.Format("{0:C0}", Model.VEH_PRECIO_INGRESO), disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">Precio de Ingreso</label>
                                        </div>

                                        <h5 class="text-condensedLight">  &nbsp;</h5>
                                        <br style="margin-top:48px" />
                                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                            @Html.TextBoxFor(a => a.VEH_STEREO_CODIGO, new { @class = "mdl-textfield__input", @typeof = "text", tabindex = "18", disabled = "disabled", @Style = "color:dimgrey" })
                                            <label class="mdl-textfield__label" for="NameAdmin" style="color: #2e6da4">CÓDIGO</label>
                                        </div>
                                        <h5 class="text-condensedLight" style="color: #2e6da4">&nbsp;</h5>
                                        <br />
                                    </div>
                                </div>

                                <p class="text-center">
                                    <button class="mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored bg-primary" name="Action" value="Edit" id="btn-EditVehic">
                                        <i class="zmdi zmdi-edit" style="display: inline-block; font: normal normal normal 14px/1"></i>
                                    </button>
                                    <button class="mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored bg-primary" name="Action" value="Delete" id="btn-DeleteVehic">
                                        <i class="zmdi zmdi-delete"></i>
                                    </button>
                                    <button class="mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored bg-primary" name="Action" value="Reserve" id="btn-ReserveVehic">
                                        <i class="zmdi zmdi-label"></i>
                                    </button>
                                    <button class="mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored bg-primary" name="Action" value="Sell" id="btn-SellVehic">
                                        <i class="zmdi zmdi-shopping-cart" ></i>
                                    </button>
                                    <div class="mdl-tooltip" for="btn-EditVehic">Editar Vehículo</div>
                                    <div class="mdl-tooltip" for="btn-DeleteVehic">Eliminar Vehículo</div>
                                    <div class="mdl-tooltip" for="btn-ReserveVehic">Reservar Vehículo</div>
                                    <div class="mdl-tooltip" for="btn-SellVehic">Vender Vehículo</div>
                                </p>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
